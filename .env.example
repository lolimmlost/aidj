# Client-side environment variables (VITE_ prefix for Vite, exposed to browser)
VITE_BASE_URL=http://localhost:3000
VITE_API_URL=http://localhost:3000

# Navidrome service (client)
VITE_NAVIDROME_URL=http://localhost:4533

# Ollama service (client)
VITE_OLLAMA_URL=http://localhost:11434
VITE_OLLAMA_MODEL=llama2

# Lidarr service (client)
VITE_LIDARR_URL=http://localhost:8686

# Server-side environment variables (not prefixed, not exposed to browser)
DATABASE_URL=postgresql://user:password@localhost:5432/aidj
AUTH_SECRET=your_random_secret_here # Generate with: openssl rand -base64 32

# Navidrome service (server)
NAV_IDROME_URL=http://localhost:4533
NAV_IDROME_USERNAME=admin
NAV_IDROME_PASSWORD=admin

# Ollama service (server)
OLLAMA_URL=http://localhost:11434

# Lidarr service (server)
LIDARR_URL=http://localhost:8686
LIDARR_API_KEY=your_lidarr_api_key_here