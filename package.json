{
  "name": "aidj-music-interface",
  "version": "1.0.0",
  "description": "AIDJ Music Library Interface - Modern music streaming and library management application",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3003 --host",
    "build": "vite build",
    "start": "node .output/server/index.mjs",
    "lint": "eslint .",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check .",
    "start:frontend": "vite dev --port 3000",
    "start:backend": "node .output/server/index.mjs",
    "check-types": "tsc --noEmit",
    "check": "npm format && npm lint && npm check-types",
    "db": "drizzle-kit",
    "db:studio": "drizzle-kit studio",
    "deps": "npm dlx taze@latest -Ilw",
    "deps:major": "npm dlx taze@latest major -Ilw",
    "ui": "npm dlx shadcn@latest",
    "auth:secret": "npm dlx @better-auth/cli@latest secret",
    "auth:generate": "npm dlx @better-auth/cli@latest generate --config ./src/lib/auth/auth.ts --y --output ./src/lib/db/schema/auth.schema.ts && prettier --write ./src/lib/db/schema/auth.schema.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "e2e": "playwright test",
    "test:ci:e2e": "playwright test tests/e2e/ --headed=false --coverage --reporter=html,line,json --project=e2e"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/react-devtools": "^0.5.4",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@tanstack/react-router": "^1.131.28",
    "@tanstack/react-router-devtools": "^1.131.28",
    "@tanstack/react-router-ssr-query": "^1.131.28",
    "@tanstack/react-start": "^1.131.28",
    "@vitejs/plugin-react": "^5.0.2",
    "better-auth": "^1.3.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.5",
    "lucide-react": "^0.542.0",
    "postgres": "^3.4.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.3.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "use-sync-external-store": "^1.2.2",
    "vite": "^7.1.3",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.52.8",
    "@eslint/js": "^9.34.0",
    "@playwright/test": "^1.48.1",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/devtools-vite": "^0.2.10",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@tanstack/eslint-plugin-router": "^1.131.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitest/coverage-istanbul": "^2.1.9",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "6.0.0-rc.2",
    "jsdom": "^23.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.3"
  }
}
